<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <Authors> Igor Bagdamyan</Authors>
        <Description>A set of computation expressions for F# base library</Description>
        <Copyright>Copyright (c) 2021 Igor Bagdamyan</Copyright>
        <PackageProjectUrl>https://github.com/En3Tho/FSharpExtensions</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/En3Tho/FSharpExtensions/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/En3Tho/FSharpExtensions</RepositoryUrl>
        <AssemblyVersion>1.0.1</AssemblyVersion>
        <FileVersion>1.0.1</FileVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.1</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <Compile Include="GenericCollectionBuilderBase.fs" />
      <Compile Include="ArrayPoolBasedBuilders.fs" />
      <Compile Include="CollectionBuilder.fs" />
    </ItemGroup>

</Project>
