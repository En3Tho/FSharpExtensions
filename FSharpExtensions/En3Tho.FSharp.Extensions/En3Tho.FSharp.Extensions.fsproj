<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <AssemblyVersion>1.0.2</AssemblyVersion>
        <FileVersion>1.0.2</FileVersion>
        <Description>A set of extensions for F# base library</Description>
        <Copyright>Copyright (c) 2021 Igor Bagdamyan</Copyright>
        <PackageProjectUrl>https://github.com/En3Tho/FSharpExtensions</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/En3Tho/FSharpExtensions/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/En3Tho/FSharpExtensions</RepositoryUrl>
        <Authors> Igor Bagdamyan</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.2</PackageVersion>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

    <ItemGroup>
      <Compile Include="Disposables.fs" />
      <Compile Include="Modules.fs" />
      <Compile Include="Extensions.fs" />
      <Compile Include="ActivePatterns.fs" />
      <Compile Include="Scanf.fs" />
    </ItemGroup>

</Project>
